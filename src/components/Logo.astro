---
import { Image } from 'astro:assets';
import logo from '@images/logo.jpg';
---

<Image
	class="laguntza-fisioterapia-logo"
	src={logo}
	alt="Laguntza fisioterapia"
>
	<style>
		.laguntza-fisioterapia-logo {
			margin: 0;
			height: 3rem;
			width: 3rem;
		}
		.laguntza-fisioterapia-logo:hover {
			text-decoration: underline;
			cursor: pointer;
		}
	</style>
</Image>
